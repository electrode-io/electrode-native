{
  "name": "ern-platform",
  "version": "1000.0.0",
  "description": "",
  "keywords": [],
  "license": "Apache-2.0",
  "scripts": {
    "setup-dev": "node setup-dev.js",
    "rebuild": "rimraf ern-*/**/dist && lerna clean --yes && lerna bootstrap --npmClient=yarn",
    "test": "lerna run test",
    "coverage": "lerna run coverage && istanbul-combine -d coverage -p summary -r lcov -r html $(find ./ern-*/.nyc_output -type f -name '*.json' -maxdepth 3)",
    "standard": "standard",
    "flow": "flow",
    "precommit": "standard && flow",
    "postmerge": "node auto-rebuild.js"
  },
  "pre-commit": [
    "standard",
    "flow"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "shelljs": "^0.7.6"
  }, 
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.2.4",
    "dir-compare": "^1.3.0",
    "dirty-chai": "^1.2.2",
    "eslint-plugin-flowtype": "^2.33.0",
    "flow-bin": "0.47.0",
    "husky": "^0.14.3",
    "install": "^0.8.7",
    "istanbul-combine": "^0.3.0",
    "lerna": "2.0.0",
    "mocha": "^3.2.0",
    "npm": "^4.4.1",
    "nyc": "^10.1.2",
    "rimraf": "^2.6.0",
    "sinon": "3.1.0",
    "standard": "^10.0.2"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeEach",
      "afterEach",
      "before",
      "after",
      "log"
    ],
    "ignore": [
      "**/dist",
      "**/ern-api-gen/",
      "**/flow-typed/",
      "**/test"
    ],
    "plugins": [
      "flowtype"
    ],
    "parser": "babel-eslint"
  },
  "engines": {
    "node": ">=4.5"
  },
  "bugs": {
    "url": "https://github.com/electrode-io/electrode-native/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/electrode-native.git"
  },
  "homepage": "http://www.electrode.io"
}
